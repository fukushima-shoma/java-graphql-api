schema {
    query: Query
    mutation: Mutation
}

type Query {
    getResource(id: String): Response
}

type Mutation {
    updateResource(inputList: [Input]): MutationResponse
}

input Input {
    id: String
    isChecked: Boolean
}

type Response {
    userList: [User]
}

type User {
    id: String
    name: String
    isCheck: Boolean
}

type MutationResponse {
    result: Boolean
}
